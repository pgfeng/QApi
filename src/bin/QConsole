#!/usr/bin/env php
<?php
require_once $GLOBALS['_composer_autoload_path'];

use QApi\Command;
use QApi\Enumeration\RunMode;

if (!defined('PROJECT_PATH')) {
    define('PROJECT_PATH', $GLOBALS['_composer_bin_dir']);
}
if (!defined('RUN_MODE')) {
    define('RUN_MODE', RunMode::DEVELOPMENT);
}
$Command = new Command();
$Command->execute();